{
  "name": "react-series-challenge",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^24.0.6",
    "@types/node": "^11.9.5",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "react": "^16.8.3",
    "react-app-rewire-workbox": "^2.0.1",
    "react-app-rewired": "^2.0.3",
    "react-dom": "^16.8.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.1.3",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^0.16.0",
    "styled-components": "^3.2.1",
    "typescript": "^3.3.3333",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}",
      "!**/src/registerServiceWorker.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "eject": "react-app-rewired eject",
    "lint": "eslint ./",
    "test": "react-app-rewired test --env=jsdom --coverage",
    "test-watch": "react-app-rewired test --env=jsdom",
    "deploy": "yarn build && firebase deploy"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-computed-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1"
  },
  "resolutions": {
    "terser": "3.14.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
